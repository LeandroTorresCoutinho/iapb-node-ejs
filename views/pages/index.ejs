    <script type="text/javascript">
        var message = '<%= message %>';
        if (message != '') {
            alert(message);
        }

        var messageEbook = '<%= messageEbook %>';
        if (messageEbook != '') {
            alert(messageEbook);
            window.open('files/ebook.docx');
        }

        var validateEbookForm = function(){
            let text = ''
            let passed = true
            if(!document.forms["ebookEmail"]["name"].value){
                text += "Campo nome é obrigatório.\n"
                passed = false
            }
            if(!document.forms["ebookEmail"]["email"].value){
                text += "Campo e-mail é obrigatório."
                passed = false
            }
            if(!document.forms["ebookEmail"]["mensagem"].value){
                text += "Campo mensagem é obrigatório."
                passed = false
            }
            if(!passed){
                alert(text);
                return false
            }else{
                loading();
                return true
            }
        }
        var validateContactForm = function(){
            let text = ''
            let passed = true
            if(!document.forms["contactForm"]["name"].value){
                text += "Campo nome é obrigatório.\n"
                passed = false
            }
            if(!document.forms["contactForm"]["email"].value){
                text += "Campo e-mail é obrigatório."
                passed = false
            }
            if(!passed){
                alert(text);
                return false
            }else{
                loading();
                return true
            }
        }

        var loading = function(){
            $('body').addClass('waiting');
            TweenMax.to('.site-loader-wrap', 1, { marginTop: 50, autoAlpha: 0, ease: Power4.easeInOut });
            document.getElementById("submitEbookForm").disabled = true;
            // document.getElementById("ebookEmail").submit();
        };
    </script>
